{
    "name": "andrey-helldar/sitemap",
    "description": "A simple sitemap generator for PHP Framework.",
    "type": "library",
    "license": "MIT",
    "homepage": "http://ai-rus.com",
    "keywords": [
        "sitemap",
        "generator",
        "framework"
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/Sitemap/issues",
        "source": "https://github.com/andrey-helldar/Sitemap"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "nesbot/carbon": "~1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~1.11",
        "phpunit/phpunit": "~4.0|~5.0",
        "sllh/php-cs-fixer-styleci-bridge": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\": "src/Sitemap/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit; ./vendor/bin/php-cs-fixer fix -v --diff --dry-run;",
        "phpunit": "./vendor/bin/phpunit;",
        "phpcs": "./vendor/bin/php-cs-fixer fix -v --diff --dry-run;"
    },
    "minimum-stability": "stable"
}
