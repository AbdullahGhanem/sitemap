{
    "name": "andrey-helldar/sitemap",
    "description": "A simple sitemap generator for PHP Framework.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/andrey-helldar/sitemap",
    "keywords": ["sitemap", "generator", "laravel", "simple", "separated files"],
    "support": {
        "issues": "https://github.com/andrey-helldar/sitemap/issues",
        "source": "https://github.com/andrey-helldar/sitemap"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": ">=5.6.9",
        "illuminate/database": "^5.4",
        "illuminate/support": "^5.4",
        "illuminate/validation": "^5.4",
        "nesbot/carbon": "^1.22",
        "symfony/http-foundation": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Sitemap\\": "src/"
        },
        "files": [
            "src/Helpers/sitemap.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\Sitemap\\ServiceProvider"
            ]
        }
    }
}
