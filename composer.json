{
    "name": "andrey-helldar/sitemap",
    "description": "A simple sitemap generator for PHP Framework.",
    "type": "library",
    "license": "MIT",
    "keywords": ["sitemap", "generator", "laravel", "simple", "seo optimized", "seo"],
    "support": {
        "issues": "https://github.com/andrey-helldar/sitemap/issues",
        "source": "https://github.com/andrey-helldar/sitemap"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-dom": "*",
        "andrey-helldar/support": "^1.11",
        "andrey-helldar/xml-core": "^1.0",
        "illuminate/support": "^5.6|^6.0",
        "nesbot/carbon": "^1.25 || ^2.0",
        "symfony/http-foundation": "^4.0",
        "vlucas/phpdotenv": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "suggest": {
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Sitemap\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\Sitemap\\ServiceProvider"
            ]
        }
    }
}
